load-target "risc-v-simple/linux"
@SIM_create_object("ram","board.sanitizer_shm", image=None, self_allocated_image_size=0x4000)
board.phys_mem.add-map board.sanitizer_shm 0x20000000 0x4000

script-branch {
    while 1 {
        bp.magic.wait-for number=28
        board.phys_mem.get-string 0x20000000
    }
}
